<% case @category %>
<% when "health" %>
	$('.health').html(<%= @new_exp %>);
<% when "stamina" %>
	$('.stamina').html(<%= @new_exp %>);
<% when "strength" %>
	$('.strength').html(<%= @new_exp %>);
<% else "intelligence" %>
	$('.intelligence').html(<%= @new_exp %>);
<% end %>
alert("Completed <%= @quest_title %>");